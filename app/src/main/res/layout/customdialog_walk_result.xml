<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/dialog_box">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >

        <TextView
            android:id="@+id/walk_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:text="23년 1월 19일"
            android:textSize="16dp"
            android:textColor="#767676"
            />

        <ImageButton
            android:id="@+id/walk_result_close"
            android:layout_width="22dp"
            android:layout_height="22dp"
            android:background="@drawable/cls_btn"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="12dp"
            android:layout_marginTop="12dp"
            app:layout_constraintTop_toTopOf="parent"
            />


        <TextView
            android:id="@+id/result_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#191919"
            android:textSize="18dp"
            android:text="코코의 산책을 완료했어!"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/walk_date"
            android:layout_marginTop="30dp"
            />

        <View
            android:id="@+id/walk_result_dvide"
            android:layout_width="1dp"
            android:layout_height="1dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:background="#00000000"/>

        <TextView
            android:id="@+id/walk_result_time_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            app:layout_constraintStart_toStartOf="@id/start1"
            app:layout_constraintEnd_toStartOf="@id/walk_result_dvide"
            app:layout_constraintTop_toBottomOf="@id/result_text"
            android:text="산책시작"
            android:textSize="14dp"
            android:textColor="#767676"
            />

        <TextView
            android:id="@+id/walk_result_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:textSize="23dp"
            android:textStyle="bold"
            android:text="00:23"
            app:layout_constraintStart_toEndOf="@id/start1"
            app:layout_constraintEnd_toStartOf="@id/walk_result_dvide"
            app:layout_constraintTop_toBottomOf="@id/walk_result_time_text"
            android:layout_marginTop="4dp"
            />

        <TextView
            android:id="@+id/walk_result_time_end_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            app:layout_constraintStart_toStartOf="@id/walk_result_dvide"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/result_text"
            android:text="산책종료"
            android:textSize="14dp"
            android:textColor="#767676"
            />
        <TextView
            android:id="@+id/walk_result_time_end"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:textSize="23dp"
            android:textStyle="bold"
            android:text="00:23"
            app:layout_constraintStart_toStartOf="@id/walk_result_dvide"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/walk_result_time_text"
            android:layout_marginTop="4dp"
            />



        <TextView
            android:id="@+id/walk_result_range_start_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="28dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="@id/walk_result_dvide"
            app:layout_constraintTop_toBottomOf="@id/walk_result_time"
            android:text="산책거리"
            android:textSize="14dp"
            android:textColor="#767676"
            />
        <TextView
            android:id="@+id/walk_result_range_start"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:textSize="23dp"
            android:textStyle="bold"
            android:text="340m"
            app:layout_constraintEnd_toEndOf="@id/walk_result_dvide"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/walk_result_range_start_text"
            android:layout_marginTop="4dp"
            />




        <TextView
            android:id="@+id/walk_result_range_end_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="28dp"
            app:layout_constraintStart_toStartOf="@id/walk_result_dvide"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/walk_result_time"
            android:text="산책시간"
            android:textSize="14dp"
            android:textColor="#767676"
            />
        <TextView
            android:id="@+id/walk_result_range_end"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:textSize="23dp"
            android:textStyle="bold"
            android:text="00:01"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@id/walk_result_dvide"
            app:layout_constraintTop_toBottomOf="@id/walk_result_range_start_text"
            android:layout_marginTop="4dp"
            />




        <View
            android:id="@+id/walk_dvide_line"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#999999"
            app:layout_constraintTop_toBottomOf="@id/walk_result_range_end"
            app:layout_constraintStart_toStartOf="@+id/start1"
            app:layout_constraintEnd_toEndOf="@+id/end1"
            android:layout_marginTop="32dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            />

        <TextView
            android:id="@+id/result_walk_coco_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toTopOf="@id/walk_dvide_line"
            android:text="오늘 코코의 산책사진"
            android:textColor="#191919"
            android:textSize="18dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/result_img" />


        <ImageView
            android:id="@+id/result_img"
            android:layout_width="match_parent"
            android:layout_height="260dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="12dp"
            android:layout_marginBottom="12dp"
            android:background="@drawable/walk_result_img"
            android:src="@drawable/gray_back"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@id/end1"
            app:layout_constraintStart_toStartOf="@id/start1"
            app:layout_constraintTop_toBottomOf="@id/result_walk_coco_text" />

        <View
            android:id="@+id/start1"
            android:layout_width="1dp"
            android:layout_height="1dp"
            android:background="#00000000"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            />
        <View
            android:id="@+id/end1"
            android:layout_width="1dp"
            android:layout_height="1dp"
            android:background="#00000000"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"

            />




    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>